#ifndef DEMO_VERSION_H
#define DEMO_VERSION_H

#define DEMO_VERSION_STRING "@DEMO_VERSION_STRING@"

#define DEMO_VERSION_MAJOR @DEMO_VERSION_MAJOR@
#define DEMO_VERSION_MINOR @DEMO_VERSION_MINOR@
#define DEMO_VERSION_PATCH @DEMO_VERSION_PATCH@

#define DEMO_VERSION(a,b,c) (((a) << 16) + ((b) << 8) + (c))
#define DEMO_VERSION_NUMBER DEMO_VERSION(@DEMO_VERSION_MAJOR@, @DEMO_VERSION_MINOR@, @DEMO_VERSION_PATCH@)

#endif  // DEMO_VERSION_H
